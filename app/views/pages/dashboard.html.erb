<div class="dashboard_container">
<h2>Dashboard</h2>

<section class="my_section">
   <%= link_to 'new service', new_service_path(current_user.id), class: "btn btn-primary" %>
  <nav class="my_nav">
    <ul>
      <li><a href="#">booking1</a></li>
      <li><a href="#">booking2</a></li>
      <li><a href="#">booking3</a></li>
    </ul>
  </nav>

   <!-- @services is no longer an array after I pulled from main branch. cannot be itterated over -->

  <article class="my_article">
    <h1>Services</h1>
   <div>   <%= @services %> </div>
    <% @services.each do |service| %>
   <!--        <div ><%= service.user.username %></div> -->
    <div>   <%= service %> </div>
          <div class="card-product">
          <img src="https://raw.githubusercontent.com/lewagon/fullstack-images/master/uikit/skateboard.jpg" />
          <div class="card-product-infos">
          <div><%= service.title %></div>
             <ul>
             <% service.bookings.each do |job| %>

              <a class="dashboard_link" href="#">
                <li class="dashboard_booking_details"><%= job.date %><p class="">
                  <%= job.user.username %></p></li>
              </a>

             <% end %>
             </ul>
          </div>
        </div>

    <% end %>
</div>
  </article>
</section>
</div>
